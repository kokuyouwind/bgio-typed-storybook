{
  "name": "@bgio-typed-storybook/lobby",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .tsx,.ts src/",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write \"**/*.+(js|json|yml|ts|tsx)\"",
    "prettier:check": "prettier --check \"**/*.+(js|json|yml|ts|tsx)\"",
    "format": "yarn run lint:fix && yarn run prettier",
    "storybook": "start-storybook -p 6006",
    "storybook:local": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test-storybook": "test-storybook",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/joy": "^5.0.0-alpha.60",
    "@mui/material": "^5.11.4",
    "boardgame.io": "^0.50.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/jest": "^0.0.10",
    "@storybook/react": "^6.5.15",
    "@storybook/test-runner": "^0.9.2",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@vitejs/plugin-react": "^3.0.1",
    "babel-loader": "^9.1.2",
    "chromatic": "^6.14.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^30.0.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "prettier": "^2.8.3",
    "typescript": "^4.9",
    "vite": "^4.0.4",
    "vite-plugin-turbosnap": "^1.0.1",
    "vitest": "^0.26.3"
  }
}
